generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(uuid())
  nickname     String   @unique
  passHash     String
  refreshToken String?
  createdAt    DateTime @default(now())

  @@map("users")
}